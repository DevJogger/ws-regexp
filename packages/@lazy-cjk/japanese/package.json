{
  "name": "@lazy-cjk/japanese",
  "version": "1.2.0",
  "description": "Util collection for Japanese text processing. Hiraganize, Katakanize, and Romanize.",
  "keywords": [
    "japanese",
    "text",
    "unicode",
    "hiragana",
    "katakana",
    "romaji",
    "romanization",
    "unihan",
    "kanji",
    "hanji",
    "CJK"
  ],
  "homepage": "https://github.com/hakatashi/japanese.js",
  "bugs": {
    "url": "https://github.com/hakatashi/japanese.js/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hakatashi/japanese.js.git"
  },
  "license": "MIT",
  "author": "hakatashi <hakatasiloving@gmail.com> (http://hakatashi.com/)",
  "main": "index.js",
  "bin": {
    "japanese": "bin/cli.js"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "yarn run test:jest",
    "test:jest": "jest --coverage",
    "npm:publish": "npm publish",
    "npm:publish:lerna": "npx lerna publish --yes --cd-version patch",
    "prepublish:lockfile": "npx sync-lockfile .",
    "prepublishOnly_": "yarn run ncu && yarn run sort-package-json && yarn run test",
    "postpublish_": "git commit -m \"chore(release): publish\" .",
    "ncu": "npx yarn-tool ncu -u",
    "sort-package-json": "npx yarn-tool sort"
  },
  "dependencies": {
    "big.js": "^5.2.2",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@types/big.js": "^4.0.5",
    "@types/lodash": "^4.14.154"
  },
  "coffeelintConfig": {
    "no_tabs": {
      "level": "ignore"
    },
    "indentation": {
      "level": "error",
      "value": 1
    }
  },
  "testling": {
    "harness": "mocha-bdd",
    "files": "test/*.js",
    "browsers": [
      "ie/6..latest",
      "chrome/latest",
      "firefox/latest"
    ]
  }
}
