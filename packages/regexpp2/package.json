{
  "name": "regexpp2",
  "version": "1.3.0",
  "description": "Regular expression parser for ECMAScript 2018.",
  "keywords": [
    "abstract",
    "annexB",
    "ast",
    "ecmascript",
    "es2015",
    "es2016",
    "es2017",
    "es2018",
    "expression",
    "parser",
    "regexp",
    "regular",
    "syntax",
    "tree",
    "validator"
  ],
  "homepage": "https://github.com/bluelovers/regexpp#readme",
  "bugs": {
    "url": "https://github.com/bluelovers/regexpp/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluelovers/regexpp.git"
  },
  "license": "MIT",
  "author": "Toru Nagashima (https://github.com/mysticatea)",
  "main": "./index.js",
  "_types": [
    "./index.d.ts"
  ],
  "directories": {
    "test": "test"
  },
  "scripts": {
    "coverage": "npx nyc npm test",
    "ncu": "npx npm-check-updates -u",
    "npm-publish": "npm publish",
    "prepublishOnly": "npm run sort-package-json && npm run test",
    "sort-package-json": "npx sort-package-json ./package.json",
    "test": "npx mocha --require ts-node/register --reporter dot --timeout 10000 \"test/**/*.ts\" --exclude \"**/*.d.ts\" --exclude \"**/visitor*\"",
    "update:ids": "ts-node scripts/update-unicode-ids.ts",
    "update:test": "ts-node scripts/update-fixtures.ts"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/mocha": "*",
    "@types/node": "*"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">=4.0.0"
  }
}
