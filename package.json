    "micromatch": "^4.0.5",
    "sort-object-keys2": "^3.0.5",
    "ts-jest": "^29.1.1",
    "uni-string": "^2.0.5",
    "ws-pkg-list": "^2.0.14",
    "xregexp": "^5.1.1",
    "zero-width": "^1.0.26"
  },
  "devDependencies": {
    "@bluelovers/tsconfig": "^1.0.34",
    "@types/big.js": "^6.2.0",
    "@types/bluebird": "^3.5.38",
    "@types/diff": "^5.0.3",
    "@types/eslint": "^8.44.2",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.5.4",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.14.197",
    "@types/micromatch": "^4.0.2",
    "@types/node": "latest",
    "@types/xregexp": "^4.4.0",
    "@types/yargs": "^17.0.24",
    "@yarn-tool/ws-find-up-paths": "*",
    "eslint": "^8.48.0",
    "tslib": ">=2"
  },
  "peerDependencies": {
    "lerna": "<7 >=6"
  }
}
